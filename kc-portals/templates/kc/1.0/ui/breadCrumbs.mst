<div class="breadcrumbs">
    <ul>
    {{#breadCrumbs}}
        {{^last}}
	        <li><a href="{{url}}">{{caption}}</a></li>
	    {{/last}}
	    {{#last}}
           <li><span>{{caption}}</span></li>
	    {{/last}}
        {{#delimeter}}
            {{delimeter}}
        {{/delimeter}}
     {{/breadCrumbs}}
    </ul>
</div>