              <div class="tabs-header">
                <a href="#" class="tab-toggle is_active" data-handler="onEventsTab" data-tab="my">
                  {{mui.mylearning.content.myEvents}}</a>
                <a href="#" class="tab-toggle" data-handler="onEventsTab" data-tab="all">
                  {{mui.mylearning.content.allEvents}}</a>
                <!--
                <a href="#" class="tab-toggle" data-handler="onEventsTab" data-tab="archive">
                  {{mui.mylearning.content.archiveEvents}}</a>-->
              </div>
            </div>

            <div class="event-filters">
              <select class="event-selector" id="eventFilter" data-handler="onSelectEventFilter">
                <option value="">{{mui.mylearning.content.event.filter.all}}</option>
                <option value="pending">{{mui.mylearning.content.event.filter.pending}}</option>
                <option value="registered">{{mui.mylearning.content.event.filter.registered}}</option>
                <option value="attended">{{mui.mylearning.content.event.filter.attended}}</option>
                <option value="missed">{{mui.mylearning.content.event.filter.missed}}</option>
                <option value="rejected">{{mui.mylearning.content.event.filter.rejected}}</option>
              </select>
            </div>

            <div class="table__row table__head table__learning__head table-head-courses">
              <div class="table__cell events--type">
                {{mui.mylearning.content.event.type}}
              </div>
              <div class="table__cell events--name">
                {{mui.mylearning.content.event.name}}
              </div>
              <div class="table__cell events--date">
                {{mui.mylearning.content.event.date}}
              </div>
              <div class="table__cell events--address">
                {{mui.mylearning.content.event.address}}
              </div>
              <div class="table__cell events--action">
                {{mui.mylearning.content.event.status}}
              </div>
              <div class="table__cell courses-table--remove"></div>
            </div>

            <div class="courses-table__content">
              {{#events}}
              <div class="table__row table__row--hovered">
                <div class="table__cell events--type">
                  {{type}}
                </div>
                <div class="table__cell events--name">
                    {{#url}}<a href="{{url}}" target="_blank">{{title}}</a>{{/url}}
                    {{^url}}{{title}}{{/url}}
                </div>
                <div class="table__cell events--date">
                  {{date_start}}
                </div>
                <div class="table__cell events--address">
                  {{address}}
                </div>
                <div class="table__cell events--action text-center">
                  {{#is_available}}
                  <a class="btn btn-sm btn-info btn-wide100" href="#" data-handler="onRegisterEvent"
                     data-course_id="{{course_id}}" data-id="{{event_id}}" data-status="{{status}}">
                    <span>{{mui.mylearning.content.event.register}}</span>
                  </a>
                  {{/is_available}}
                  {{#is_attended}}
                  {{#certificate_number}}
                  <a class="btn btn-sm btn-wide100" href="#" data-handler="onShowCertificate"
                     data-id="{{certificate_number}}" data-status="{{status}}">
                    <img src="{{IMG}}certificate-icon.png"/> <span>{{mui.mylearning.content.event.attended}}</span>
                  </a>
                  {{/certificate_number}}
                  {{^certificate_number}}
                  <span class="btn btn-sm btn-link events--approve btn-wide100" href="#" data-id="{{event_id}}"
                        data-status="{{status}}">
                                    <span>{{mui.mylearning.content.event.attended}}</span>
                                  </span>
                  {{/certificate_number}}
                  {{/is_attended}}
                  {{#is_missed}}
                  <!--
                  <a class="btn btn-sm btn-warning btn-wide100" href="#" data-id="{{event_id}}"
                     data-status="{{status}}">
                    <span>{{mui.mylearning.content.event.missed}}</span>
                  </a>
                -->
                    <span class="full-width text-danger">{{mui.mylearning.content.event.missed}}</span>

                  {{/is_missed}}

                  {{#is_review}}
                  <!--
                  <a class="btn btn-sm btn-danger btn-wide100" href="#" data-handler="onUnRegisterEvent"
                     data-id="{{event_id}}" data-status="{{status}}">
                    <span>{{mui.mylearning.content.event.unregister}}</span>
                  </a>
                -->
                    <span class="full-width text-warning">{{mui.mylearning.content.event.filter.pending}}</span>
                  {{/is_review}}


                  {{#is_approved}}
                  <span class="btn btn-sm btn-link events--approve btn-wide100" href="#" data-id="{{event_id}}"
                        data-status="{{status}}">
                                <span>{{mui.mylearning.event.approved}}</span>
                              </span>
                  {{/is_approved}}

                  {{#is_rejected}}
                  <span class="events--rejected btn-wide100" data-id="{{event_id}}" data-status="{{status}}">
                                <span>{{mui.mylearning.event.rejected}}</span>
                              </span>
                  {{/is_rejected}}
                  <span class="event-message--ml">{{event_message}}</span>
                </div>
                <div class="table__cell  courses-table--remove">
                  &nbsp;
                </div>

              </div>
              {{/events}}
              {{^events}}

              <div class="table__row">
                <div class="table__cell">
                  {{mui.NothingToShow}}
                </div>
              </div>

              {{/events}}

              {{#showMoreBtn}}

              <div class="table__row">
                <div class="table__cell center-align">
                  <a class="tr-btn" href="#" data-handler="onShowMore">{{mui.ShowMore}} ({{showMoreBtnCount}}
                    )</a>
                </div>
              </div>

              {{/showMoreBtn}}
            </div>
          </div>


        </div>

      </div>
    </div>


  </div>
</div>

