<div class="inner-hero inner-hero--learningLibrary">
    <div class="cnt-wide ll-hero-wrapper">
        <div class='ll-hero-content'>
            <div class='hero-slider-container'>
                <div id="start" class="hero-slider-wrapper active">
                    <h1 class="inner-hero__title">{{heroTitle}}</h1>
                    <h3>{{heroTitleH3}}</h3>
                    <p class="inner-hero__subtitle">{{heroSubtitle}}</p>
                </div>
                <div id="hero-business" class="hero-slider-wrapper">
                    <h2 class="inner-hero__title">{{tabbusinessTitle}}</h2>
                    <h3>{{tabbusinesSubtitle}}</h3>

                    <p class="inner-hero__subtitle">{{tabbusinesDescription}}</p>
                    <ul class='business'>
                        {{#list_business}}
                        <li class="{{class}}"><a href="{{url}}">{{name}}</a></li>
                        {{/list_business}}
                    </ul>
                </div>
                <div id="hero-computer" class="hero-slider-wrapper">
                    <h2 class="inner-hero__title">{{tabcomputerTitle}}</br>{{tabcomputerSubtitle}}</h2>
                    <p class="inner-hero__subtitle">{{tabcomputerDescription}}</p>
                    <ul class='computer'>
                        {{#list_computer}}
                        <li class="{{class}}"><a href="{{url}}">{{name}}</a></li>
                        {{/list_computer}}
                    </ul>
                </div>
                <div id="hero-safety" class="hero-slider-wrapper">
                    <h2 class="inner-hero__title">{{tabsafetyTitle}}</br>{{tabsafetySubtitle}}</h2>
                    <p class="inner-hero__subtitle">{{tabsafetyDescription}}</p>
                    <ul class='safety'>
                        {{#list_safety}}
                        <li class="{{class}}"><a href="{{url}}">{{name}}</a></li>
                        {{/list_safety}}
                    </ul>
                </div>
                <div id="hero-compliance" class="hero-slider-wrapper">
                    <h2 class="inner-hero__title">{{tabcomplianceTitle}}</br>{{tabcomplianceSubtitle}}</h2>
                    <p class="inner-hero__subtitle">{{tabcomplianceDescription}}</p>
                    <ul class='compliance'>
                        {{#list_compliance}}
                        <li class="{{class}}"><a href="{{url}}">{{name}}</a></li>
                        {{/list_compliance}}
                    </ul>
                </div>
                <div id="hero-finance" class="hero-slider-wrapper">
                    <h2 class="inner-hero__title">{{tabfinanceTitle}}</br>{{tabfinanceSubtitle}}</h2>
                    <p class="inner-hero__subtitle">{{tabfinanceDescription}}</p>
                    <ul class='finance'>
                        {{#list_finance}}
                        <li class="{{class}}"><a href="{{url}}">{{name}}</a></li>
                        {{/list_finance}}
                    </ul>
                </div>
            </div>
            <div class="button-wrapper">
                <div class="lms-button"><a href="{{site.url_with_lang}}signUp/type/business" class="lms-free">{{mui.buttonLabel.tryItFree}}</a></div>
                <div class="lms-button"><a data-handler="onScrollNode" data-to="#library_tabs" class="lms-features">{{learnMore}}</a></div>
            </div>
            <ul class="dot-wrapper">
                <li><span class="circle" data-handler="onDotClick" data-wrapper="business"></span><span class="tooltip">{{businessTitle}}</span></li>
                <li><span class="circle" data-handler="onDotClick" data-wrapper="computer"></span><span class="tooltip">{{computerTitle}}</span></li>
                <li><span class="circle" data-handler="onDotClick" data-wrapper="safety"></span><span class="tooltip">{{safetyTitle}}</span></li>
                <li><span class="circle" data-handler="onDotClick" data-wrapper="compliance"></span><span class="tooltip">{{complianceTitle}}</span></li>
                <li><span class="circle" data-handler="onDotClick" data-wrapper="finance"></span><span class="tooltip">{{financeTitle}}</span></li>
            </ul>
        </div>
        <div class="ll-hero-library-images">
            <a class="header-selector" data-handler="onChangeHeroContent" data-wrapper="business">
                <div class="block">
                    <div class="block-1 business">
                        <div class="overlay"></div>
                        <div class="centered">{{businessTitle}}</div>
                    </div>
                </div>
            </a>
            <a class="header-selector" data-handler="onChangeHeroContent" data-wrapper="computer">
                <div class="block">
                    <div class="block-1 computer">
                        <div class="overlay"></div>
                        <div class="centered">{{computerTitle}}</div>
                    </div>
                </div>
            </a>
            <a class="header-selector" data-handler="onChangeHeroContent" data-wrapper="safety">
                <div class="block">
                    <div class="block-1 safety">
                        <div class="overlay">
                        </div><div class="centered">{{safetyTitle}}</div>
                    </div>
                </div>
            </a>
            <a class="header-selector" data-handler="onChangeHeroContent" data-wrapper="compliance">
                <div class="block">
                    <div class="block-1 compliance">
                        <div class="overlay"></div>
                        <div class="centered">{{complianceTitle}}</div>
                    </div>
                </div>
            </a>
            <a class="header-selector" data-handler="onChangeHeroContent" data-wrapper="finance">
                <div class="block">
                    <div class="block-1 finance">
                        <div class="overlay"></div>
                        <div class="centered">{{financeTitle}}</div>
                    </div>
                </div>
            </a>
        </div>
    </div>
</div>


<section class="library-devices">
