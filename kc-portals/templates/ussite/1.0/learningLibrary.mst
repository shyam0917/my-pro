<section class="ll-cta">
    <div class="cnt-wide">
        <div class="cta">
            <div class="cta-title" >{{RibbonTitle}}</div>
            <div class="cta-subtitle">{{{RibbonDescription}}}</div>
            <div class="header-button">
                <a class="btn sign-up freetrial" href="{{site.url_with_lang}}signUp/type/business">{{mui.buttonLabel.tryItFree}}</a>
            </div>
        </div>
    </div>
</section>

<section class="ll-contact">
    <div class="cnt-narrow">
        <div class="ll-contact-inner">
            <h2>{{contactTitle}}</h2>
            <p>{{contactSubtitle}}</p>
            <div class="ll-contact-form">
                <form id="contactUs" class="ll-contact__form" data-handler="onSubmitForm" method="post" action="">
            <div class="ll-contact__row">
                <input
                        type="text"
                        class="ll-contact__input"
                        value=""
                        id="name_f"
                        name="name_f"
                        placeholder="{{mui.pageContent.formlabel.name}}"
                        required="true"
                        data-invalidMessage="{{mui.form.requiredField.invalidName}}"
                />
                <input
                        type="email"
                        class="ll-contact__input"
                        value=""
                        id="email"
                        name="email"
                        placeholder="{{mui.pageContent.formlabel.email}}"
                        required="true"
                        data-invalidMessage="{{mui.form.requiredField.invalidEmail}}"
                        data-invalidMessageWrongType="{{mui.form.requiredField.invalidTypeEmail}}"
                />
            </div>
            <div class="ll-contact__row">
                <select class="ll-contact__subject"
                        value=""
                        id="subject"
                        name="subject"
                        placeholder="{{mui.pageContent.formlabel.subject}}"
                        required="true"
                        data-invalidMessage="{{mui.form.requiredField.invalidSubject}}">
                    <option value="Learning Library">{{contactSubject}} {{contactLearning}}</option>
                    <option value="LMS">{{contactSubject}} {{contactLms}}</option>
                    <option value="Content Creation">{{contactSubject}} {{contactCustom}}</option>
                    <option value="General">{{contactSubject}} {{contactGeneral}}</option>
                </select>
            </div>
            <div class="ll-contact__row">
                <textarea
                        class="ll-contact__textarea"
                        id="comments"
                        name="comments"
                        placeholder="{{mui.pageContent.formlabel.message}}"
                        required="true"
                        data-invalidMessage="{{mui.form.requiredField.invalidHowWeCanHelp}}"
                ></textarea>
            </div>
            <input class="ll-contact__btn btn" type="submit"  name="submit" value="{{mui.buttonLabel.submit}}" class="button" rel="submit">
        </form>
            </div>

        </div>
    </div>
</section>


<section class="bottom-links">
    <div class="cnt-wide">
        <div class="bot-columns">
            <div class="">
                <a href="{{site.url_with_lang}}lms"><div class="bottom-link__img">
                    <img src="{{VIEW}}images/bottom-link_LMS--v{{site.LocalStorageStamp}}.jpg" style="width:100%;
                height:auto" alt="">
